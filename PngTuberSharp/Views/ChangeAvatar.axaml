<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:i="https://github.com/projektanker/icons.avalonia"
             x:Class="PngTuberSharp.Views.ChangeAvatar">
	<StackPanel Spacing="5" >
		<TextBlock FontSize="24">Change Avatar</TextBlock>

		<StackPanel Orientation="Horizontal">
			<Button Click="ChangeOpen">Change Open</Button>
			<TextBlock x:Name="fileOpen" Margin="5,5,5,5" VerticalAlignment="Center"></TextBlock>
		</StackPanel>
		<StackPanel Orientation="Horizontal">
			<Button Click="ChangeClosed">Change Close</Button>
			<TextBlock x:Name="fileClosed" Margin="5,5,5,5" VerticalAlignment="Center"></TextBlock>
		</StackPanel>
	</StackPanel>
</UserControl>
